import network

wlan = network.WLAN(network.STA_IF)
wlan.active(True)

f = open("usr/net/ssid.cfg", "r")
ssid = f.read()
f.close()

f = open("usr/net/pass.cfg", "r")
passw = f.read()
f.close()

print("Connecting to \""+ssid+"\" with password \""+passw+"\"")
wlan.connect(ssid, passw)
print("Connected: "+str(wlan.isconnected()))
print(wlan.ifconfig())
